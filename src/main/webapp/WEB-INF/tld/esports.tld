<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="https://jakarta.ee/xml/ns/jakartaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
        https://jakarta.ee/xml/ns/jakartaee/web-jsptaglibrary_3_0.xsd"
        version="3.0">

    <tlib-version>1.0</tlib-version>
    <short-name>esports</short-name>
    <uri>http://esports.betting/functions</uri>

    <function>
        <name>formatDateTime</name>
        <function-class>lk.esports.betting.utils.JSPFunctions</function-class>
        <function-signature>java.lang.String formatDateTime(java.time.LocalDateTime, java.lang.String)</function-signature>
    </function>

    <function>
        <name>formatDateTimeDefault</name>
        <function-class>lk.esports.betting.utils.JSPFunctions</function-class>
        <function-signature>java.lang.String formatDateTime(java.time.LocalDateTime)</function-signature>
    </function>

    <function>
        <name>formatDate</name>
        <function-class>lk.esports.betting.utils.JSPFunctions</function-class>
        <function-signature>java.lang.String formatDate(java.time.LocalDateTime)</function-signature>
    </function>

    <function>
        <name>formatTime</name>
        <function-class>lk.esports.betting.utils.JSPFunctions</function-class>
        <function-signature>java.lang.String formatTime(java.time.LocalDateTime)</function-signature>
    </function>

    <function>
        <name>formatShort</name>
        <function-class>lk.esports.betting.utils.JSPFunctions</function-class>
        <function-signature>java.lang.String formatShort(java.time.LocalDateTime)</function-signature>
    </function>
</taglib>